{
  "name": "musicblocks",
  "version": "3.4.1",
  "main": "electron-main.js",
  "description": "A musical microworld",
  "repository": {
    "url": "https://github.com/sugarlabs/musicblocks.git"
  },
  "license": "AGPL-3.0 License",
  "bugs": {
    "url": "https://github.com/sugarlabs/musicblocks/issues"
  },
  "amd": {},
  "scripts": {
    "lint": "eslint js/**/*.js",
    "serve": "http-server -a 127.0.0.1 -p 3000 --gzip --brotli",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "cypress:run": "cypress run",
    "electron": "electron .",
    "dist": "electron-builder",
    "build": "gulp"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@eslint/js": "^9.19.0",
    "cypress": "^15.9.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^15.14.0",
    "gulp": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-prettier": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "nodemon": "^3.1.9"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.28",
    "autoprefixer": "^10.4.20",
    "compression": "^1.8.1",
    "cssnano": "^7.1.2",
    "express": "^5.2.1",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^10.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "postcss": "^8.5.6",
    "sass": "^1.84.1",
    "http-server": "^14.1.1",
    "i18next": "^25.3.2",
    "i18next-http-backend": "^3.0.2",
    "tone": "^15.1.22"
  },
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || ^24.0.0"
  },
  "build": {
    "appId": "com.musicblocks.app",
    "productName": "Music Blocks",
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}